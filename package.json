{
  "name": "ng2-web-storage",
  "version": "0.0.1",
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/core": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.5",
    "@types/core-js": "^0.9.32",
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.38",
    "@types/webpack": "^1.12.33",
    "awesome-typescript-loader": "^2.2.1",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-webpack": "^1.8.0",
    "rxjs": "5.0.0-beta.6",
    "ts-helpers": "^1.1.1",
    "typescript": "^2.0.0-beta",
    "typings": "^1.3.2",
    "webpack": "^2.1.0-beta.21",
    "zone.js": "^0.6.6"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.5"
  },
  "scripts": {
    "flow.clean": "node node_modules/del-cli/cli typings \"src/**/*.+(js|d.ts|js.map)\" \"index.+(js|d.ts|js.map)\"",
    "flow.install:typings": "typings install",
    "flow.compile": "npm run flow.install:typings && npm run flow.compile:common",
    "flow.compile:common": "tsc -p tsconfig.json",
    "dev.compile": "npm run flow.clean && npm run flow.compile && npm pack",
    "test": "karma start",
    "build": "webpack"
  }
}
