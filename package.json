{
  "name": "ng2-web-storage",
  "version": "0.0.1",
  "devDependencies": {
    "@angular/core": "^2.0.0-rc.5",
    "del-cli": "^0.2.0",
    "rxjs": "5.0.0-beta.6",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "zone.js": "^0.6.6"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.5"
  },
  "scripts": {
    "flow.clean": "node node_modules/del-cli/cli typings \"src/**/*.+(js|d.ts|js.map)\" \"index.+(js|d.ts|js.map)\"",
    "flow.install:typings": "typings install",
    "flow.compile": "npm run flow.install:typings && npm run flow.compile:common",
    "flow.compile:common": "tsc -p tsconfig.json",
    "dev.compile": "npm run flow.clean && npm run flow.compile && npm pack"
  }
}
